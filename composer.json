{
    "name": "kreait/firebase-bundle",
    "description": "Symfony Bundle for the Firebase Admin SDK",
    "keywords": ["firebase", "google", "sdk", "api", "database", "symfony", "bundle", "symfony-bundle"],
    "homepage": "https://github.com/kreait/firebase-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Jérôme Gamez",
            "homepage": "https://github.com/jeromegamez"
        }
    ],
    "require": {
        "kreait/firebase-php": "^4.34",
        "symfony/dependency-injection": "^2.8|^3.4|^4.0",
        "symfony/config": "^2.8|^3.4|^4.0",
        "symfony/http-kernel": "^2.8|^3.4|^4.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.3.4"
    },
    "autoload": {
        "psr-4": {
            "Kreait\\Firebase\\Symfony\\Bundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    }
}
